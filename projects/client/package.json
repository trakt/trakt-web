{
  "name": "client",
  "version": "0.0.1",
  "type": "module",
  "license": "MIT",
  "scripts": {
    "pre:dev": "deno task i18n:web:watch",
    "dev": "concurrently 'deno task pre:dev' 'vite dev'",
    "dev:staging": "concurrently 'deno task pre:dev' 'cross-env IS_STAGING=true vite dev'",
    "dev:contrib": "concurrently 'deno task pre:dev' 'cross-env  IS_CONTRIB=true vite dev'",
    "prebuild": "deno task i18n:web",
    "build": "vite build",
    "prebuild:preview": "deno task prebuild",
    "build:preview": "IS_PREVIEW=true vite build",
    "preview": "vite preview",
    "check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
    "check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
    "lint": "eslint .",
    "pretest:unit": "deno task prebuild",
    "test:unit": "vitest",
    "pretest:cov": "deno task prebuild",
    "test:cov": "vitest --coverage",
    "test:e2e": "cucumber-js -c ./cucumber.mjs",
    "i18n:web": "deno task i18n:generate ./i18n --platforms web",
    "i18n:web:watch": "deno task i18n:generate:watch ./i18n --platforms web",
    "i18n:sync": "deno run --allow-read --allow-write .scripts/sync-i18n.ts",
    "i18n:resolve": "deno run --allow-read --allow-write .scripts/resolve-i18n.ts",
    "i18n:traktify": "deno run --allow-read --allow-write --allow-net --allow-env .scripts/traktify-i18n.ts",
    "i18n:cleanse": "deno run --allow-read --allow-write --allow-env .scripts/cleanse-i18n.ts",
    "i18n:generate": "deno run --allow-read --allow-write i18n/generator/cli.ts -i i18n/meta/ -o i18n/",
    "i18n:l10n": "deno run --allow-read --allow-write i18n/generator/l10n.ts -i i18n/ios/Localizable.xcstrings -o i18n/ios/L10n.swift",
    "i18n:generate:watch": "deno run --allow-read --allow-write i18n/generator/cli.ts -i i18n/meta/ -o i18n/ --watch"
  },
  "devDependencies": {
    "@cucumber/cucumber": "12.5.0",
    "@eslint/js": "^9.39.2",
    "@google/generative-ai": "0.24.1",
    "@playwright/test": "1.57.0",
    "@sveltejs/adapter-auto": "7.0.0",
    "@sveltejs/adapter-cloudflare": "7.2.4",
    "@sveltejs/vite-plugin-svelte": "6.2.4",
    "@testing-library/jest-dom": "6.9.1",
    "@testing-library/svelte": "5.3.1",
    "@testing-library/user-event": "14.6.1",
    "@types/eslint": "9.6.1",
    "@types/serviceworker": "0.0.176",
    "@vite-pwa/sveltekit": "1.1.0",
    "@vitest/coverage-istanbul": "3.2.4",
    "concurrently": "9.2.1",
    "cross-env": "10.1.0",
    "eslint": "9.39.2",
    "eslint-plugin-svelte": "3.14.0",
    "globals": "17.0.0",
    "jsdom": "26.1.0",
    "msw": "2.12.7",
    "playwright": "1.57.0",
    "prettier": "3.7.4",
    "prettier-plugin-svelte": "3.4.1",
    "sass-embedded": "1.97.2",
    "svelte-check": "4.3.5",
    "typescript": "5.8.3",
    "typescript-eslint": "8.53.0",
    "vite": "7.3.1",
    "vite-plugin-pwa": "1.2.0",
    "vitest": "3.2.4"
  },
  "dependencies": {
    "@inlang/paraglide-js": "2.8.0",
    "@libn/fuzzy": "npm:@jsr/libn__fuzzy@^0.4.0",
    "@sentry/sveltekit": "10.33.0",
    "@sveltejs/kit": "^2.49.4",
    "@tanstack/svelte-query": "5.90.2",
    "@tanstack/svelte-query-devtools": "5.90.2",
    "@tanstack/svelte-query-persist-client": "5.90.2",
    "@trakt/api": "npm:@jsr/trakt__api@0.3.0",
    "@ts-rest/core": "3.52.1",
    "@use-gesture/vanilla": "10.3.1",
    "date-fns": "4.1.0",
    "fflate": "^0.8.2",
    "firebase": "12.7.0",
    "flowbite-svelte": "^1.31.0",
    "idb-keyval": "6.2.2",
    "marked": "^17.0.1",
    "oidc-client-ts": "3.4.1",
    "plyr": "^3.8.4",
    "resolve-accept-language": "3.1.15",
    "rxjs": "^7.8.2",
    "svelte": "^5.46.3",
    "svelte-confetti": "^2.3.2",
    "ts-retry": "^6.0.0",
    "typesense": "^2.1.0",
    "workbox-expiration": "7.4.0",
    "workbox-precaching": "7.4.0",
    "workbox-routing": "7.4.0",
    "workbox-strategies": "7.4.0",
    "zod": "3.25.76",
    "zod-to-json-schema": "3.25.1"
  }
}
