{
  "name": "client",
  "version": "0.0.1",
  "type": "module",
  "license": "MIT",
  "scripts": {
    "pre:dev": "deno task i18n:web:watch",
    "dev": "concurrently 'deno task pre:dev' 'vite dev'",
    "dev:staging": "concurrently 'deno task pre:dev' 'IS_STAGING=true vite dev'",
    "dev:contrib": "concurrently 'deno task pre:dev' 'IS_CONTRIB=true vite dev'",
    "prebuild": "deno task i18n:web",
    "build": "vite build",
    "prebuild:preview": "deno task prebuild",
    "build:preview": "IS_PREVIEW=true vite build",
    "preview": "vite preview",
    "check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
    "check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
    "lint": "eslint .",
    "pretest:unit": "deno task prebuild",
    "test:unit": "vitest",
    "pretest:cov": "deno task prebuild",
    "test:cov": "vitest --coverage",
    "test:e2e": "cucumber-js -c ./cucumber.mjs",
    "i18n:web": "deno task i18n:generate ./i18n --platforms web",
    "i18n:web:watch": "deno task i18n:generate:watch ./i18n --platforms web",
    "i18n:sync": "deno run --allow-read --allow-write .scripts/sync-i18n.ts",
    "i18n:resolve": "deno run --allow-read --allow-write .scripts/resolve-i18n.ts",
    "i18n:traktify": "deno run --allow-read --allow-write --allow-net --allow-env .scripts/traktify-i18n.ts",
    "i18n:cleanse": "deno run --allow-read --allow-write --allow-env .scripts/cleanse-i18n.ts",
    "i18n:generate": "deno run --allow-read --allow-write i18n/generator/cli.ts -i i18n/meta/ -o i18n/",
    "i18n:l10n": "deno run --allow-read --allow-write i18n/generator/l10n.ts -i i18n/ios/Localizable.xcstrings -o i18n/ios/L10n.swift",
    "i18n:generate:watch": "deno run --allow-read --allow-write i18n/generator/cli.ts -i i18n/meta/ -o i18n/ --watch"
  },
  "devDependencies": {
    "@cucumber/cucumber": "12.2.0",
    "@google/generative-ai": "0.24.1",
    "@playwright/test": "1.55.0",
    "@svelte-put/qr": "2.1.0",
    "@sveltejs/adapter-auto": "6.1.0",
    "@sveltejs/adapter-cloudflare": "7.2.3",
    "@sveltejs/enhanced-img": "0.6.0",
    "@sveltejs/kit": "2.37.0",
    "@sveltejs/vite-plugin-svelte": "5.1.1",
    "@testing-library/jest-dom": "6.8.0",
    "@testing-library/svelte": "5.2.8",
    "@testing-library/user-event": "14.6.1",
    "@types/eslint": "9.6.1",
    "@types/serviceworker": "0.0.152",
    "@vite-pwa/sveltekit": "1.0.0",
    "@vitest/coverage-istanbul": "3.2.4",
    "concurrently": "9.1.2",
    "eslint": "9.34.0",
    "eslint-plugin-svelte": "3.11.0",
    "flowbite-svelte": "1.13.3",
    "globals": "16.3.0",
    "jsdom": "26.1.0",
    "marked": "15.0.12",
    "msw": "2.7.3",
    "playwright": "1.55.0",
    "postcss-nested": "7.0.2",
    "prettier": "3.6.2",
    "prettier-plugin-svelte": "3.4.0",
    "sass-embedded": "1.91.0",
    "svelte": "5.38.6",
    "svelte-check": "4.3.1",
    "ts-retry": "^6.0.0",
    "typescript": "5.8.3",
    "typescript-eslint": "8.41.0",
    "vite": "6.3.5",
    "vite-plugin-pwa": "1.0.3",
    "vitest": "3.2.4"
  },
  "dependencies": {
    "@inlang/paraglide-js": "2.2.0",
    "@sentry/sveltekit": "10.7.0",
    "@tanstack/svelte-query": "5.85.5",
    "@tanstack/svelte-query-devtools": "5.85.5",
    "@tanstack/svelte-query-persist-client": "5.85.5",
    "@trakt/api": "npm:@jsr/trakt__api@0.2.33",
    "@ts-rest/core": "3.52.1",
    "@use-gesture/vanilla": "10.3.1",
    "date-fns": "4.1.0",
    "firebase": "12.2.0",
    "idb-keyval": "6.2.2",
    "oidc-client-ts": "3.3.0",
    "resolve-accept-language": "3.1.13",
    "rxjs": "^7.8.2",
    "typesense": "^2.1.0",
    "workbox-expiration": "7.3.0",
    "workbox-precaching": "7.3.0",
    "workbox-routing": "7.3.0",
    "workbox-strategies": "7.3.0",
    "zod": "3.25.76",
    "zod-to-json-schema": "3.24.6"
  }
}
